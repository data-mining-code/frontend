    "test": "standard | snazzy"
  },
  "dependencies": {
    "choo": "6.6.1",
    "choo-devtools": "2.3.3",
    "sheetify": "7.0.0",
    "tachyons": "4.9.1"
  },
  "devDependencies": {
    "browserify": "15.2.0",
    "concurrently": "3.5.1",
    "envify": "4.1.0",
    "serve": "6.4.9",
    "snazzy": "7.0.0",
    "standard": "10.0.3",
    "watchify": "3.10.0"
  }
}
